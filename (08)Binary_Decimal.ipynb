   "source": [
    "def binary_to_decimal(binary):\n",
    "    decimal = 0\n",
    "    power = 0\n",
    "    for digit in reversed(binary):\n",
    "        decimal += int(digit) * (2 ** power)\n",
    "        power += 1\n",
    "    return decimal\n",
    "\n",
    "binary = input(\"Enter a binary number: \")\n",
    "print(\"Decimal:\", binary_to_decimal(binary))\n"
   ]
  }
